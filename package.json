  "bugs": {
    "url": "https://github.com/insightjs/stock-wizard/issues"
  },
  "homepage": "https://github.com/insightjs/stock-wizard#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "cookie-parser": "^1.4.3",
    "express": "^4.16.4",
    "jsonwebtoken": "^8.4.0",
    "pg": "^7.7.1",
    "pg-promise": "^8.5.3",
    "react": "^16.7.0-alpha.0",
    "react-dom": "^16.7.0-alpha.0",
    "victory": "^31.0.2",
    "yahoo-finance": "^0.3.5",
    "node-timers": "0.0.2"
  },
